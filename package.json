{
  "name": "launcher-desktop-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@tauri-apps/api": "^1.0",
    "@material-ui/core": "^4.11.2",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.57",
    "@types/node": "^14.14.16",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "darkreader": "^4.9.26",
    "react": "^17.0.1",
    "react-beautiful-dnd": "^13.0.0",
    "react-dom": "^17.0.1",
    "react-flip-move": "^3.0.4",
    "react-scripts": "^4.0.1",
    "typescript": "^4.1.3",
    "uuid": "^7.0.3",
    "wouter": "^2.7.0"
  },
  "scripts": {
    "dev": "tauri dev",
    "nvmrc": "node -v > .nvmrc",
    "postinstall": "yarn nvmrc",
    "predev": "yarn nvmrc",
    "tauri": "tauri",
    "start": "vite",
    "rls": "dotenv-load tauri build",
    "build": "vite build",
    "preview": "vite preview",
    "serve": "vite preview",
    "preinstall": "yarn global add sloc @tauri-apps/cli",
    "sloc": "sloc src src-tauri/src",
    "update": "concurrently \"yarn upgrade \" \"cd src-tauri && cargo update && cargo clean\""
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@tauri-apps/cli": "^1.2.3",
    "@vitejs/plugin-react": "^2.1.0",
    "rollup-plugin-visualizer": "^5.6.0",
    "cross-env": "^7.0.3",
    "concurrently": "^7.0.0",
    "dotenv-load": "^2.0.1",
    "@types/material-ui": "^0.21.8",
    "@types/react-beautiful-dnd": "^13.0.0",
    "vite": "^3.1.0",
    "toml": "^3.0.0",
    "vite-plugin-top-level-await": "^1.2.1"
  },
  "resolutions": {}
